   "source": [
    "t = [[0.5, 0.5, 0.2], [0.2, 0.7, 0.5], [0.4, 0.1, 0.5]]\n",
    "g = [0]*3\n",
    "p = [0.6, 0.4, 0.7]\n",
    "\n",
    "for i in range(3):\n",
    "    for j in range(3):\n",
    "        g[i] = max(g[i], t[j][i]*p[j]);\n",
    "        \n",
    "print('Values for each', g)\n",
    "# index + 1 because we start from 0\n",
    "print('P: a{}'.format(g.index(max(g)) + 1))\n"
   ]
